public class Main
{
    public static void main(String[] args) {
        int param1 = 5; 
        int param2 = 7; 
        int param3 = 3; 
        
       int result = suma(param1, param2, param3);
       System.out.println(result);
    }
    
    public static int suma(int a, int b, int c){
        return a+b+c;
    }
}


